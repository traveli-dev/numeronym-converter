{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "outDir": "build",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "moduleResolution": "node",
  },
  "baseUrl": "./",
  "paths": {
    "@/*": [
      "src/*"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}